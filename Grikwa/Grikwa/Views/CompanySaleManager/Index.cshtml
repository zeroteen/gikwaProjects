            @Html.DisplayFor(modelItem => item.Employee.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Order.OrderNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SaleTime)
