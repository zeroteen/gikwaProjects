@{
    ViewBag.Title = "Manage Password";
}

<h2>@ViewBag.Title.</h2>

<div class="row">
    <div class="col-md-12">
        @if (ViewBag.HasLocalPassword)
        {
            @Html.Partial("_ChangePasswordPartial")
        }
        else
        {
            @Html.Partial("_SetPasswordPartial")
        }
    </div>
</div>
<p>@Html.ActionLink("Back to Manage Account", "Manage")</p>
