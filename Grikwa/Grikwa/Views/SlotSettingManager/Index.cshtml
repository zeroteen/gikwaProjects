@model IEnumerable<Grikwa.Models.SlotSetting>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StartTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MinutesRange)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaxQuantity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinutesRange)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaxQuantity)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SlotSettingID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SlotSettingID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SlotSettingID })
        </td>
    </tr>
}

</table>
